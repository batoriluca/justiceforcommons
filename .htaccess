RewriteEngine on

#ErrorDocument 404 https://fattakse.com/404

RewriteRule ^law-update/(.*)?$ law-update.php?slug=$1 [QSA,L]
RewriteRule ^supreme-court-update/(.*)?$ supreme-court-update.php?slug=$1 [QSA,L]

RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.*)$ $1.php [NC,L]

